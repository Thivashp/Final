@model IEnumerable<WebApplication5.Models.CricketResults>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HomeTeam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AwayTeam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HomeRuns)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AwayRuns)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HOversFaced)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AOversFaced)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HomeWickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AwayWickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HomeTeam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AwayTeam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HomeRuns)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AwayRuns)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HOversFaced)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AOversFaced)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HomeWickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AwayWickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.mId }) |
            @Html.ActionLink("Details", "Details", new { id=item.mId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.mId })
        </td>
    </tr>
}

</table>
